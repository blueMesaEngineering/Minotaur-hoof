<% provide(:title, "New model") %>

<h1>New Model</h1>
<center>

	<%= form_with scope: :url_data_model, url: url_data_models_path, local: true do |form| %>
		<p>
			Please enter a URL. The URL must include the full path, including the "http://" and the full suffix such as ".com", ".edu", etc.
		</p>
		<p>
			<%= form.label :address, "URL:" %>
			<%= form.text_field :address %>
		</p>

		<p>
			<%= form.submit %>
		</p>
	<% end %>
</center>